{
  "name": "@utterance/core",
  "version": "0.0.1",
  "description": "Client-side semantic endpointing. Know when they're done talking.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "models"
  ],
  "scripts": {
    "start": "npm run build:sdk && run-p build:sdk:watch dev test:watch",
    "dev": "next dev",
    "build": "npm run build:sdk && next build",
    "build:sdk": "tsup src/index.ts --format esm,cjs --dts --clean --tsconfig tsconfig.sdk.json",
    "build:sdk:watch": "tsup src/index.ts --watch --format esm,cjs --dts --tsconfig tsconfig.sdk.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "voice",
    "speech",
    "endpointing",
    "vad",
    "turn-detection",
    "audio",
    "ml",
    "onnx",
    "web-audio",
    "real-time"
  ],
  "author": "Utterance Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nizh0/Utterance.git"
  },
  "homepage": "https://utterance.dev",
  "bugs": {
    "url": "https://github.com/nizh0/Utterance/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "eslint": "^9.0.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.4.0",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.2.0",
    "tsup": "^8.0.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@react-three/fiber": "^9.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fumadocs-core": "^16.6.3",
    "fumadocs-mdx": "^14.2.7",
    "fumadocs-ui": "^16.6.3",
    "lucide-react": "^0.574.0",
    "next": "^16.1.6",
    "onnxruntime-web": "^1.20.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.1",
    "three": "^0.183.0"
  }
}
